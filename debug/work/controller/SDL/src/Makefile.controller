all:
	mkdir -p code
	cd code && opengeode --toAda ../*.pr
	asn1scc -fp AUTO -typePrefix asn1Scc -o code -equal -Ada  code/controller_datamodel.asn
	cd code && gnat make controller
c:
	mkdir -p code_c
	cd code_c && opengeode --toC ../*.pr
	asn1scc -fp AUTO -ig -renamePolicy 3 -typePrefix asn1Scc -o code_c -equal -c  code/controller_datamodel.asn
	cd code_c && gcc -c *.c
clean:
	rm -rf code code_c